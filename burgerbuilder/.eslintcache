eprecatedRules":"60"},{"filePath":"107","messages":"108","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"60"},{"filePath":"109","messages":"110","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"60"},{"filePath":"111","messages":"112","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"60"},{"filePath":"113","messages":"114","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0,"usedDeprecatedRules":"60"},{"filePath":"115","messages":"116","errorCount":0,"warningCount":0,"fixableErrorCount":0,"fixableWarningCount":0},"F:\\Coding Ninjas Web Development\\burgerbuilder\\src\\index.js",[],["117","118"],"F:\\Coding Ninjas Web Development\\burgerbuilder\\src\\App.js",[],"F:\\Coding Ninjas Web Development\\burgerbuilder\\src\\registerServiceWorker.js",[],"F:\\Coding Ninjas Web Development\\burgerbuilder\\src\\components\\Layout\\Layout.js",[],"F:\\Coding Ninjas Web Development\\burgerbuilder\\src\\containers\\Checkout\\Checkout.js",[],"F:\\Coding Ninjas Web Development\\burgerbuilder\\src\\containers\\Orders\\Orders.js",[],"F:\\Coding Ninjas Web Development\\burgerbuilder\\src\\containers\\BurgerBuilder\\BurgerBuilder.js",["119"],"import React, { Component } from 'react';\r\n\r\nimport Aux from '../../hoc/Auxilliary';\r\nimport Burger from '../../components/Burger/Burger';\r\nimport BuildControls from '../../components/Burger/BuildControls/BuildControls';\r\nimport Modal from '../../components/UI/Modal/Modal';\r\nimport OrderSummary from '../../components/Burger/OrderSummary/OrderSummary';\r\nimport axios from '../../axios-orders';\r\nimport Spinner from '../../components/UI/Spinner/Spinner';\r\nimport withErrorHandler from '../../hoc/WItherrorHandler/withErrorHandler';\r\n\r\n\r\n//WE ARE DECLARING IT OUT OF CLASS BECAUSE IT'S A CONSTANT\r\nconst INGREDIENT_PRICES={\r\n    salad: 0.5,\r\n    cheese: 0.4,\r\n    meat: 1.3,\r\n    bacon: 0.7 \r\n}\r\n\r\nclass BurgerBuilder extends Component {\r\n    // constructor(props) {\r\n    //     super(props);\r\n    //     this.state = {...}\r\n    // }\r\n    state = {\r\n        ingredients: null,\r\n        totalPrice:0,\r\n        purchasable:false,\r\n        orderable:false,\r\n        loading:false\r\n    }\r\n\r\n    componentDidMount(){\r\n        console.log(this.props);\r\n        axios.get('https://cart-30e0a.firebaseio.com/ingredients.json')\r\n            .then(Response=>{\r\n                this.setState({ingredients:Response.data});\r\n            })\r\n    }\r\n\r\n    updatePurchaseState(ingredients){\r\n\r\n        //will create an array of the string i.e cheese,salad,meet but we need count\r\n        const sum=Object.keys(ingredients)\r\n        .map(igKey=>{\r\n            return ingredients[igKey]\r\n        })\r\n        .reduce((sum,el)=>{\r\n            return sum+el;\r\n        },0);\r\n        this.setState({purchasable: sum>0})\r\n    }\r\n\r\n    addIngredientHandler=(type)=>{\r\n        \r\n        //dealing with count\r\n        const oldCount=this.state.ingredients[type];\r\n        const updatedCount=oldCount+1;\r\n        const updatedIngredients={\r\n            ...this.state.ingredients\r\n        };\r\n        updatedIngredients[type]=updatedCount;\r\n        \r\n        //dealing with price\r\n        const priceAddition=INGREDIENT_PRICES[type];\r\n        const oldPrice=this.state.totalPrice;\r\n        const newPrice=oldPrice+priceAddition;\r\n\r\n        this.setState({ingredients:updatedIngredients,totalPrice:newPrice});\r\n        this.updatePurchaseState(updatedIngredients);\r\n    }\r\n\r\n    removeIngredientHandler=(type)=>{\r\n        //dealing with count\r\n        const oldCount=this.state.ingredients[type];\r\n        if(oldCount==0){\r\n            return -1;\r\n        }\r\n        const updatedCount=oldCount-1;\r\n        const updatedIngredients={\r\n            ...this.state.ingredients\r\n        };\r\n        updatedIngredients[type]=updatedCount;\r\n        \r\n        //dealing with price\r\n        const priceAddition=INGREDIENT_PRICES[type];\r\n        const oldPrice=this.state.totalPrice;\r\n        const newPrice=oldPrice-priceAddition;\r\n\r\n        this.setState({ingredients:updatedIngredients,totalPrice:newPrice});\r\n        this.updatePurchaseState(updatedIngredients);\r\n    }\r\n    \r\n    //this is for the sake of orderSummary could be visible\r\n    orderHandler=()=>{\r\n        this.setState({orderable:true})\r\n    }\r\n\r\n    //so that summary could be removed on clicking anywhere else on window\r\n    cancelOrder=()=>{\r\n        this.setState({orderable:false})\r\n    }\r\n\r\n    continueOrder=()=>{\r\n        // this.props.history.push('/Checkout');\r\n        const queryParams=[];\r\n        for (let i in this.state.ingredients){\r\n            queryParams.push(encodeURIComponent(i)+'='+encodeURIComponent(this.state.ingredients[i]));\r\n        }\r\n        queryParams.push('price='+this.state.totalPrice);\r\n        const queryString=queryParams.join('&');\r\n        this.props.history.push({\r\n            pathname:'/checkout',\r\n            search:'?'+queryString\r\n        });\r\n    }\r\n\r\n    render () {\r\n        const disabledInfo={\r\n            ...this.state.ingredients\r\n        }\r\n        for(let key in disabledInfo){\r\n            disabledInfo[key] = disabledInfo[key]<=0\r\n        }\r\n\r\n        let orderSummary=null;\r\n\r\n        let burger= <Spinner />\r\n\r\n        if (this.state.ingredients){\r\n            burger=(\r\n                <Aux>\r\n                    <Burger ingredients={this.state.ingredients} />\r\n                        <BuildControls\r\n                        ingredientAdded={this.addIngredientHandler} \r\n                        ingredientRemoved={this.removeIngredientHandler}\r\n                        disabled={disabledInfo}\r\n                        purchasable={this.state.purchasable}\r\n                        price={this.state.totalPrice}\r\n                        order={this.orderHandler}\r\n                        />\r\n                </Aux>\r\n            );\r\n            orderSummary=<OrderSummary \r\n            ingredients={this.state.ingredients}\r\n            orderCanceled={this.cancelOrder}\r\n            orderContinued={this.continueOrder}\r\n            totalPrice={this.state.totalPrice}\r\n            /> \r\n        }\r\n\r\n        if(this.state.loading){\r\n            orderSummary=<Spinner />\r\n        }\r\n\r\n        return (\r\n            <Aux>\r\n                <Modal show={this.state.orderable} modalClosed={this.cancelOrder}>    {/* we will make it class based for shouldcomponentupdate */}\r\n                    {orderSummary}\r\n                </Modal>\r\n               {burger}\r\n            </Aux>\r\n        );\r\n    }\r\n}\r\n\r\nexport default withErrorHandler(BurgerBuilder,axios);","F:\\Coding Ninjas Web Development\\burgerbuilder\\src\\components\\Navigation\\Toolbar\\Toolbar.js",[],"F:\\Coding Ninjas Web Development\\burgerbuilder\\src\\hoc\\Auxilliary.js",[],"F:\\Coding Ninjas Web Development\\burgerbuilder\\src\\components\\Navigation\\SideDrawer\\SideDrawer.js",[],"F:\\Coding Ninjas Web Development\\burgerbuilder\\src\\axios-orders.js",[],"F:\\Coding Ninjas Web Development\\burgerbuilder\\src\\components\\Order\\CheckoutSummary\\CheckoutSummary.js",[],"F:\\Coding Ninjas Web Development\\burgerbuilder\\src\\containers\\Checkout\\ContactData\\ContactData.js",[],"F:\\Coding Ninjas Web Development\\burgerbuilder\\src\\components\\Burger\\Burger.js",[],"F:\\Coding Ninjas Web Development\\burgerbuilder\\src\\components\\Burger\\BuildControls\\BuildControls.js",[],"F:\\Coding Ninjas Web Development\\burgerbuilder\\src\\components\\UI\\Modal\\Modal.js",["120","121"],"import React, { Component } from 'react';\r\nimport classes from './Modal.module.css';\r\nimport Aux f