s.state.ingredients\n        };\n        updatedIngredients[type]=updatedCount;\n        \n        //dealing with price\n        const priceAddition=INGREDIENT_PRICES[type];\n        const oldPrice=this.state.totalPrice;\n        const newPrice=oldPrice+priceAddition;\n\n        this.setState({ingredients:updatedIngredients,totalPrice:newPrice});\n        this.updatePurchaseState(updatedIngredients);\n    }\n\n    removeIngredientHandler=(type)=>{\n        //dealing with count\n        const oldCount=this.state.ingredients[type];\n        if(oldCount==0){\n            return -1;\n        }\n        const updatedCount=oldCount-1;\n        const updatedIngredients={\n            ...this.state.ingredients\n        };\n        updatedIngredients[type]=updatedCount;\n        \n        //dealing with price\n        const priceAddition=INGREDIENT_PRICES[type];\n        const oldPrice=this.state.totalPrice;\n        const newPrice=oldPrice-priceAddition;\n\n        this.setState({ingredients:updatedIngredients,totalPrice:newPrice});\n        this.updatePurchaseState(updatedIngredients);\n    }\n    \n    //this is for the sake of orderSummary could be visible\n    orderHandler=()=>{\n        this.setState({orderable:true})\n    }\n\n    //so that summary could be removed on clicking anywhere else on window\n    cancelOrder=()=>{\n        this.setState({orderable:false})\n    }\n\n    continueOrder=()=>{\n        alert('Madafaka! Have the best burger of your life meat good, cream good...');\n    }\n\n    render () {\n        const disabledInfo={\n            ...this.state.ingredients\n        }\n        for(let key in disabledInfo){\n            disabledInfo[key] = disabledInfo[key]<=0\n        }\n        return (\n            <Aux>\n                <Modal show={this.state.orderable} modalClosed={this.cancelOrder}>\n                    <OrderSummary \n                    ingredients={this.state.ingredients}\n                    orderCanceled={this.cancelOrder}\n                    orderContinued={this.continueOrder}\n                    totalPrice={this.state.totalPrice}\n                    /> \n                </Modal>\n                <Burger ingredients={this.state.ingredients} />\n                <BuildControls\n                ingredientAdded={this.addIngredientHandler} \n                ingredientRemoved={this.removeIngredientHandler}\n                disabled={disabledInfo}\n                purchasable={this.state.purchasable}\n                price={this.state.totalPrice}\n                order={this.orderHandler}\n                />\n            </Aux>\n        );\n    }\n}\n\nexport default BurgerBuilder;","F:\\Coding Ninjas Web Development\\burgerbuilder\\src\\components\\Burger\\Burger.js",[],"F:\\Coding Ninjas Web Development\\burgerbuilder\\src\\registerServiceWorker.js",[],"F:\\Coding Ninjas Web Development\\burgerbuilder\\src\\components\\Layout\\Layout.js",[],"F:\\Coding Ninjas Web Development\\burgerbuilder\\src\\components\\Burger\\BurgerIngredient\\BurgerIngredient.js",[],"F:\\Coding Ninjas Web Development\\burgerbuilder\\src\\hoc\\Auxilliary.js",[],"F:\\Coding Ninjas Web Development\\burgerbuilder\\src\\components\\Burger\\BuildControls\\BuildControls.js",[],"F:\\Coding Ninjas Web Development\\burgerbuilder\\src\\components\\Burger\\BuildControls\\BuildControl\\BuildControl.js",[],"F:\\Coding Ninjas Web Development\\burgerbuilder\\src\\components\\UI\\Modal\\Modal.js",[],"F:\\Coding Ninjas Web Development\\burgerbuilder\\src\\components\\Burger\\OrderSummary\\OrderSummary.js",[],"F:\\Coding Ninjas Web Development\\burgerbuilder\\src\\components\\UI\\Backdrop\\Backdrop.js",[],"F:\\Coding Ninjas Web Development\\burgerbuilder\\src\\components\\UI\\Button\\Button.js",[],"F:\\Coding Ninjas Web Development\\burgerbuilder\\src\\components\\Navigation\\Toolbar\\Toolbar.js",[],"F:\\Coding Ninjas Web Development\\burgerbuilder\\src\\components\\Logo\\Logo.js",[],"F:\\Coding Ninjas Web Development\\burgerbuilder\\src\\compo